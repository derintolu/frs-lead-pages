{
  "name": "frs/lead-pages",
  "description": "Lead generation landing page builder with FluentForms integration",
  "autoload": {
    "psr-4": {
      "FRSLeadPages\\": "includes/",
      "FRSLeadPages\\Database\\": "database/"
    },
    "files": [
      "includes/functions.php"
    ]
  },
  "require": {
    "prappo/wp-eloquent": "^3.0",
    "automattic/jetpack-autoloader": "^3.0"
  },
  "minimum-stability": "RC",
  "config": {
    "allow-plugins": {
      "automattic/jetpack-autoloader": true
    }
  }
}
